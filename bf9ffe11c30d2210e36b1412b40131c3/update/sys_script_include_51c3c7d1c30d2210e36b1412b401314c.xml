        <api_name>x_1642211_it_ass_0.GetEmailAddressForITAM</api_name>
        <caller_access/>
        <client_callable>true</client_callable>
        <description>Script Include to return an email address.  The calling client-side script passes a sys_id for a User table record.</description>
        <mobile_callable>false</mobile_callable>
        <name>GetEmailAddressForITAM</name>
        <sandbox_callable>true</sandbox_callable>
        <script><![CDATA[var GetEmailAddressForITAM = Class.create();
GetEmailAddressForITAM.prototype = Object.extendsObject(global.AbstractAjaxProcessor, {

    getEmail: function() {
        var userRecord = new GlideRecord("sys_user");
        userRecord.get(this.getParameter('sysparm_userID'));
        return userRecord.email + '';
    },
    type: 'GetEmailAddressForITAM'
});]]></script>
